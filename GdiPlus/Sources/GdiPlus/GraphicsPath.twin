        Function GetTypes(types() As Byte) As GpStatus
            Dim count&
            Dim status As Any = Me.GetPointCount(count)
            If status = Ok Then
                ReDim types(1 To count)
                status = Me.GetTypes2(types(1), count)
            End If
            Return status
        End Function

