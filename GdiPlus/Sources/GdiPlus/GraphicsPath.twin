    End Function
    
    Function Transform(matrix As Matrix) As GpStatus
        If matrix IsNot Nothing Then
            Return SetStatus(pa.Transform(GetNative(matrix)))
        Else
            Return Ok
        End If
    End Function
    
    'The GetWorldBounds rectangle may not be the tightest bounds.
    
    Function GetWorldBounds(bounds As GpRectF, matrix As Matrix, pen As Pen) As GpStatus
