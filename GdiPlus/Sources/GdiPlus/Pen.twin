Module PenModule
    Type GpPen
        Native As LongPtr
    End Type
    
    Function GpPen() As GpPen
        GpPen.Native = 0
    End Function
End Module

Class Pen
    Inherits GdiPlusBase
    Dim pn As GpPen
    Dim LastResult As GpStatus
    
    Protected Property Get Native() As GpPen
        Return pn
    End Property
End Class
